{
	"name": "speedsouls",
	"version": "1.0.0",
	"scripts": {
		"fmt": "prettier --write . && pnpm run -r --parallel fmt",
		"fmt.check": "prettier --check . && pnpm run -r --parallel fmt.check",
		"lint": "eslint . --ext .ts,.cjs && pnpm run -r --parallel lint"
	},
	"devDependencies": {
		"@types/eslint": "^8.21.3",
		"@types/node": "^18.15.9",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.36.0",
		"prettier": "^2.8.7",
		"typescript": "5.0.2"
	}
}
